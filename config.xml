<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ohalloran.NeuroCalc" version="1.6" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>NeuroCalc</name>
    <description>
        Calculator for Nernst and Goldman Hogkin Katz equations.
    </description>
    <author email="dmohalloran28@gmail.com" href="www.ohalloranlab.net">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <platform name="android">
        <icon density="mdpi" src="www/res/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="www/res/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="www/res/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="www/res/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="www/res/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <icon height="152" platform="ios" src="www/res/icon/Icon-App-152x152.png" width="152" />
        <icon height="120" platform="ios" src="www/res/icon/Icon-App-120x120.png" width="120" />
        <icon height="20" platform="ios" src="www/res/icon/Icon-App-20x20@1x.png" width="20" />
        <icon height="20" platform="ios" src="www/res/icon/Icon-App-20x20@2x.png" width="20" />
        <icon height="20" platform="ios" src="www/res/icon/Icon-App-20x20@3x.png" width="20" />
        <icon height="29" platform="ios" src="www/res/icon/Icon-App-29x29@1x.png" width="29" />
        <icon height="29" platform="ios" src="www/res/icon/Icon-App-29x29@2x.png" width="29" />
        <icon height="29" platform="ios" src="www/res/icon/Icon-App-29x29@3x.png" width="29" />
        <icon height="40" platform="ios" src="www/res/icon/Icon-App-40x40@1x.png" width="40" />
        <icon height="40" platform="ios" src="www/res/icon/Icon-App-40x40@2x.png" width="40" />
        <icon height="40" platform="ios" src="www/res/icon/Icon-App-40x40@3x.png" width="40" />
        <icon height="57" platform="ios" src="www/res/icon/Icon-App-57x57@1x.png" width="57" />
        <icon height="57" platform="ios" src="www/res/icon/Icon-App-57x57@2x.png" width="57" />
        <icon height="60" platform="ios" src="www/res/icon/Icon-App-60x60@1x.png" width="60" />
        <icon height="60" platform="ios" src="www/res/icon/Icon-App-60x60@2x.png" width="60" />
        <icon height="60" platform="ios" src="www/res/icon/Icon-App-60x60@3x.png" width="60" />
        <icon height="72" platform="ios" src="www/res/icon/Icon-App-72x72@1x.png" width="72" />
        <icon height="72" platform="ios" src="www/res/icon/Icon-App-72x72@2x.png" width="72" />
        <icon height="76" platform="ios" src="www/res/icon/Icon-App-76x76@1x.png" width="76" />
        <icon height="76" platform="ios" src="www/res/icon/Icon-App-76x76@2x.png" width="76" />
        <icon height="76" platform="ios" src="www/res/icon/Icon-App-76x76@3x.png" width="76" />
        <icon height="83.5" platform="ios" src="www/res/icon/Icon-App-83.5x83.5@2x.png" width="83.5" />
        <icon height="50" platform="ios" src="www/res/icon/Icon-Small-50x50@1x.png" width="50" />
        <icon height="50" platform="ios" src="www/res/icon/Icon-Small-50x50@2x.png" width="50" />
        <icon height="1024" platform="ios" src="www/res/icon/itunesArtwork@2x.png" width="1024" />
        <icon height="512" platform="ios" src="www/res/iTunesArtwork@1x.png" width="512" />
        <icon height="1024" platform="ios" src="www/res/iTunesArtwork@2x.png" width="1024" />
        <icon height="1536" platform="ios" src="www/res/iTunesArtwork@3x.png" width="1536" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-battery-status" spec="~1.2.4" />
    <plugin name="cordova-plugin-camera" spec="~2.4.1" />
    <plugin name="cordova-plugin-media-capture" spec="~1.4.3" />
    <plugin name="cordova-plugin-device" spec="~1.1.6" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-file" spec="~4.3.3" />
    <plugin name="cordova-plugin-geolocation" spec="~2.4.3" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.7" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.1" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.3" />
    <plugin name="cordova-plugin-vibration" spec="~2.1.5" />
    <gap:config-file parent="NSPhotoLibraryUsageDescription" platform="ios">
        <string>For uploading receipt images</string>
    </gap:config-file>
    <engine name="android" spec="~7.1.1" />
    <engine name="ios" spec="~4.5.5" />
</widget>
