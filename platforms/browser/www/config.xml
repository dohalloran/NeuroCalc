<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ohalloran.NeuroCalc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>NeuroCalc</name>
    <description>
        Calculator for Nernst and Goldman Hogkin Katz equations.
    </description>
    <author email="dmohalloran28@gmail.com" href="www.ohalloranlab.net">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
